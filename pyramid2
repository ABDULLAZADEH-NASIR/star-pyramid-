public class Main {
    public static void main(String[] args) {

    int setirsayi=5;
    int eded=1;
    for(int setir=0;setir<setirsayi;setir++){

        for(int bosluq=0;bosluq<setirsayi-setir;bosluq++){
            System.out.print(" ");

        }
        for(int reqem=0;reqem<1+(setir*2);reqem++){
            System.out.print(eded);


        }
        System.out.println();
        eded++;
    }
  }
}

